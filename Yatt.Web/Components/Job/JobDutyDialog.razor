<MudDialog Style="width:500px">
    <DialogContent>
        <MudTextField T="string" Lines="5" Variant="Variant.Outlined" For="@(()=>dutyDto.Duty)" Label="Duty or responsiblity" @bind-Value="dutyDto.Duty" />
